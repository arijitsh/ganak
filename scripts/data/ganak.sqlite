  unkn_sz INT,
  cache_del_time FLOAT,
  cache_miss_rate FLOAT,
  bdd_called INT,
  sat_called INT,
  sat_rst INT,
  restarts INT,
  cubes_orig INT,
  cubes_final INT,
  mem_out INT,
  gates_extended INT,
  gates_extend_t INT,
  padoa_extended INT,
  padoa_extend_t INT
);
. mode csv
. import -skip 1 mydata.csv data
