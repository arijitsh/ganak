DROP TABLE IF EXISTS data;
CREATE TABLE data (
  dirname STRING NOT NULL,
  fname STRING NOT NULL,

  ganak_time FLOAT,
  ganak_tout_time FLOAT,
  ganak_mem_MB FLOAT,
  ganak_call TEXT NOT NULL,
  ganak_ver TEXT NOT NULL,
  confls INT,
  decs INT,
  comps INT,
  td_width INT,
  td_time FLOAT
);
. mode csv
. import -skip 1 mydata.csv data
UPDATE data SET ganak_time = NULL WHERE ganak_time = '';
UPDATE data SET confls = NULL WHERE confls = '';
UPDATE data SET decs = NULL WHERE decs = '';
UPDATE data SET comps = NULL WHERE comps = '';
UPDATE data SET td_width = NULL WHERE td_width = '';
UPDATE data SET td_time = NULL WHERE td_time = '';
