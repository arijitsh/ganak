DROP TABLE IF EXISTS data;
CREATE TABLE data (
  dirname STRING NOT NULL,
  fname STRING NOT NULL,

  ganak_time FLOAT,
  ganak_tout_time FLOAT,
  ganak_mem_MB FLOAT,
  ganak_call TEXT NOT NULL,
  ganak_ver TEXT NOT NULL,
  conflicts INT,
  decisionsK INT,
